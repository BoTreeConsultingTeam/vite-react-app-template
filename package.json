{
  "name": "vite-template",
  "version": "1.0.0",
  "main": "index.js",
  "description": "Starter kit for Tntra web application",
  "author": {
    "name": "Farhan Mithani",
    "url": "https://github.com/farhan-tntra"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.4",
    "@testing-library/user-event": "^12.1.10",
    "@vitejs/plugin-react": "^3.1.0",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.36.0",
    "formik": "^2.2.9",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-bootstrap-typeahead": "^6.0.2",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.3",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "sass": "^1.60.0",
    "vite": "^4.2.1",
    "web-vitals": "^1.0.1",
    "yup": "^1.0.2"
  },
  "scripts": {
    "start-local": "env-cmd -f .env react-scripts start",
    "dev": "env-cmd -f .env vite",
    "dev-local": "vite",
    "serve": "yarn run build && vite preview --port 3000",
    "lint": "eslint . --ext .ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "build": "vite build",
    "test": "jest --config ./jest.config.js",
    "test:watch": "yran run test -- --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.5.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "license": "MIT"
}
